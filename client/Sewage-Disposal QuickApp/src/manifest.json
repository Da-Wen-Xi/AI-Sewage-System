{
  "package": "com.ynu.edu.cn.sewage",
  "name": "智能物联乡村污水处理",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/Common/pic/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.media"
    },
    {
      "name": "system.request"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "debug",
    "data": {
      "shiroToken": "",
      "equipmentList": []
    }
  },
  "router": {
    "entry": "login",
    "pages": {
      "login": {
        "component": "index"
      },
      "main": {
        "component": "index"
      },
      "job/home": {
        "component": "index"
      },
      "job/create": {
        "component": "index"
      },
      "job/detail": {
        "component": "index"
      },
      "job/list": {
        "component": "index"
      },
      "data": {
        "component": "index"
      }
    },
    "widgets": {
      "card/eqmonitor": {
        "name": "eqmonitor",
        "description": "设备监控卡片",
        "component": "index",
        "path": "/card/eqmonitor",
        "features": [
          {
            "name": "system.network"
          },
          {
            "name": "system.storage"
          }
        ],
        "minPlatformVersion": 1041,
        "targetManufactorys": [
          "vivo",
          "xiaomi"
        ],
        "params": {
          "title": "Equipment Monitor",
          "height": "33.3%",
          "enableFold": false
        }
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#09ba07",
    "titleBarTextColor": "#ffffff",
    "menu": true,
    "pages": {
      "login": {
        "titleBarText": "登录页",
        "menu": false
      },
      "main": {
        "titleBarText": "智能物联乡村污水处理"
      },
      "job/home": {
        "titleBarText": "工单中心"
      },
      "job/create": {
        "titleBarText": "工单创建"
      },
      "job/detail": {
        "titleBarText": "工单处理"
      },
      "job/list": {
        "titleBarText": "工单列表"
      },
      "data": {
        "titleBarText": "日累计数据"
      }
    }
  }
}
